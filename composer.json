{
  "name": "cspray/architectural-decision",
  "description": "A library to keep track of architectural decisions with PHP annotations.",
  "license": ["MIT"],
  "bin": ["bin/architectural-decisions"],
  "autoload": {
    "psr-4": {
      "Cspray\\ArchitecturalDecision\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\ArchitecturalDecision\\": "tests"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-dom": "*",
    "cspray/annotated-target": "^0.2"
  },
  "require-dev": {
    "cspray/phinal": "^1.0",
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.24"
  }
}
